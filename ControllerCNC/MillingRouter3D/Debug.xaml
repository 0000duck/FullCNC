<Window x:Class="MillingRouter3D.Debug"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Debug" Height="700" Width="700" WindowState="Maximized">
    <Grid>
        <DockPanel LastChildFill="True">
            <Slider Name="SnapshotSelector" Height="69" DockPanel.Dock="Top" ValueChanged="SnapshotSelector_ValueChanged"/>
            <ScrollViewer Name="Viewer">
            </ScrollViewer>
        </DockPanel>
    </Grid>
</Window>
